@model IEnumerable<CognitiveServices.Models.GetImageModel>
<a asp-action="AddImage" class="btn btn-lg btn-primary">Add Image</a>

<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-xl-3 col-lg-4 col-md-6 col-12
             rounded rounded-3 border border-3 p-3">
            <figure class="figure">
                <img src="data:image/*;base64, @(Convert.ToBase64String(item.ImageData))"
                     class="card-img-bottom"
                     alt="@item.ImageData"/>
            </figure>
        </div>
    }

</div>

